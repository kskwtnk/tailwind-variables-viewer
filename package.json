{
  "name": "tailwind-variables-viewer",
  "version": "0.0.0",
  "description": "View Tailwind CSS v4 theme variables in your browser",
  "type": "module",
  "bin": {
    "tailwind-variables-viewer": "./dist-cli/cli/index.js"
  },
  "files": [
    "cli",
    "lib",
    "dist",
    "dist-cli"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:cli": "tsc",
    "prepare": "bun run build:cli && bun run build",
    "preview": "vite preview",
    "test:build-scenarios": "node scripts/build-test-scenarios.mjs",
    "test:scenarios": "bun run test:build-scenarios && bunx tsx test-scenarios.ts",
    "test": "bun run test:scenarios"
  },
  "keywords": [
    "tailwindcss",
    "tailwind",
    "css",
    "theme",
    "variables",
    "viewer"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.2",
    "open": "^11.0.0",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "vite": "^7.3.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/node": "^24.0.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
