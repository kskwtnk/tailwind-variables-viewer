{
  "name": "tailwind-variables-viewer",
  "version": "1.0.0",
  "description": "View Tailwind CSS v4 theme variables in your browser",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/kskwtnk/tailwind-variables-viewer"
  },
  "bugs": {
    "url": "https://github.com/kskwtnk/tailwind-variables-viewer/issues"
  },
  "homepage": "https://github.com/kskwtnk/tailwind-variables-viewer#readme",
  "bin": {
    "tailwind-variables-viewer": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build:cli": "tsc",
    "build": "bun scripts/build.ts",
    "check": "biome check --write .",
    "dev": "bun run build && bun run build:cli && node dist/cli/index.js -c dev/sample.css -o",
    "format": "biome format --write .",
    "lint": "biome lint .",
    "prepublishOnly": "bun run build && bun run build:cli",
    "test": "bun test"
  },
  "keywords": [
    "tailwindcss",
    "tailwind",
    "css",
    "theme",
    "variables",
    "viewer"
  ],
  "author": "Keisuke Watanuki (kskwtnk)",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^5.0.0",
    "commander": "^14.0.2",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "^1.3.5",
    "@types/node": "^24.0.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  }
}
